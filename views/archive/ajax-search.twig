<div class=""  data-template="ajax-search.twig">
{% if pages is not empty %}
  <table class="the-search-table uk-table uk-table-hover uk-table-divider uk-table-justify uk-table-middle uk-margin-remove">
    <tbody>
    {% for post in pages %}
      {% include 'tease-ajax.twig' %}
    {% endfor %}
    </tbody>
  </table>
{% else %}
  <p class="uk-text-emphasis uk-margin-remove">Sorry your search cannot find anything... Please Try again</p>
{% endif %}
</div>